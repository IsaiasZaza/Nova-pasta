model User {
  id         Int      @id @default(autoincrement())
  name       String
  Senha      String
  cpf        String   @unique
  phone      String
  email      String   @unique
  cep        String
  street     String
  number     String
  complement String?
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
}

model Produto {
  id         Int      @id @default(autoincrement())
  nome       String
  price      Float
  quantidade Int
  image      String?
}
